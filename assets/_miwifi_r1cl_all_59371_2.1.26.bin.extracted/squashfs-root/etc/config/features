package 'features'

config 'features' 'system'
    option 'shutdown' '0'
    option 'downloadlogs' '0'
    option 'i18n' '0'

config 'features' 'wifi'
    option 'wifi24' '1'
    option 'wifi50' '0'
    option 'wifiguest' '0'

config 'features' 'apmode'
    option 'wifiapmode' '1'
    option 'lanapmode' '1'

config 'features' 'apps'
    option 'apptc' '0'
    option 'qos' '0'

config 'features' 'hardware'
    option 'usb' '0'
    option 'disk' '0'